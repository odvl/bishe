<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iocm.administrator.freetime" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.iocm.freetime.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.iocm.freetime.activity.LoginActivity"
            android:label="Yunxuan"></activity>
        <activity android:name="com.iocm.freetime.activity.RegisterActivity"
            android:label="Yunxuan"></activity>

        <activity android:name="com.iocm.freetime.activity.TaskDetailActivity"
            android:label="@string/yunxuan"></activity>
        <activity android:name="com.iocm.freetime.activity.FindTaskActivity"
            android:label="@string/yunxuan"></activity>
        <activity android:name="com.iocm.freetime.activity.ChangeUserNameActivity"
            android:label="@string/yunxuan"></activity>

        <activity android:name="com.iocm.freetime.activity.UserApplyedActivity"
            android:label="@string/yunxuan"></activity>

        <activity android:name="com.iocm.freetime.activity.UserReleasedActivity"
            android:label="@string/yunxuan"></activity>

        <activity android:name="com.iocm.freetime.user_service.Msg"
            android:label="@string/yunxuan"></activity>
    </application>

</manifest>
